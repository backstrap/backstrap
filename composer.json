{
    "name": "backstrap/backstrap",
    "description": "The Backstrap JavaScript library provides a layer of functionality tying together Bootstrap and Backbone.",
    "keywords": ["backbone", "bootstrap", "browser", "client", "UI"],
    "type": "component",
    "homepage": "http://backstrap.github.io/backstrap",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/backstrap/backstrap/issues",
        "source": "https://github.com/backstrap/backstrap/tree/master"
    },
    "authors": [
        {
            "name": "Kevin Perry",
            "email": "perry@princeton.edu"
        }
    ],
    "require": {
        "components/backbone" : "~1.0",
        "components/bootstrap": "~3.1",
        "moment/moment": "dev-develop"
    },
    "require-dev": {
        "components/bootstrap-default": "dev-master"
    },
    "suggest": {
        "components/bootstrap-default": "dev-master"
    },
    "extra": {
        "component": {
            "scripts": [
                "js/requirejs_setup.js",
                "js/backstrap.js",
                "js/input_event.js",
                "js/backstrap/dispatcher.js",
                "js/backstrap/Model.js",
                "js/backstrap/Collection.js",
                "js/backstrap/CollectionView.js",
                "js/backstrap/HasAlternativeProperty.js",
                "js/backstrap/HasError.js",
                "js/backstrap/HasFocus.js",
                "js/backstrap/HasFormLabel.js",
                "js/backstrap/HasGlyph.js",
                "js/backstrap/HasModel.js",
                "js/backstrap/List.js",
                "js/backstrap/Badge.js",
                "js/backstrap/BasicNavbar.js",
                "js/backstrap/Button.js",
                "js/backstrap/Calendar.js",
                "js/backstrap/Checkbox.js",
                "js/backstrap/Context.js",
                "js/backstrap/Container.js",
                "js/backstrap/DatePicker.js",
                "js/backstrap/Dropdown.js",
                "js/backstrap/Glyph.js",
                "js/backstrap/Grid.js",
                "js/backstrap/Label.js",
                "js/backstrap/Link.js",
                "js/backstrap/List.js",
                "js/backstrap/Menu.js",
                "js/backstrap/ModelView.js",
                "js/backstrap/navbar.js",
                "js/backstrap/NavPills.js",
                "js/backstrap/NavTabs.js",
                "js/backstrap/Panel.js",
                "js/backstrap/RadioGroup.js",
                "js/backstrap/Select.js",
                "js/backstrap/Table.js",
                "js/backstrap/TextArea.js",
                "js/backstrap/TextField.js",
                "js/backstrap/TimePicker.js",
                "js/requirejs_cleanup.js"
            ],
            "styles": [
               "css/backstrap.css"
            ],
            "shim": {
                "deps": [
                     "jquery",
                     "underscore",
                     "bootstrap",
                     "backbone",
                     "moment"
                ]
            }
        }
    },
    "config": {
        "component": {
            "noGlobal": false
        }
    }
}
