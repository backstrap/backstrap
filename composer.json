{
    "name": "backstrap/backstrap",
    "description": "The Backstrap JavaScript library provides a layer of functionality tying together Bootstrap and Backbone.",
    "keywords": ["backbone", "bootstrap", "browser", "client", "UI"],
    "type": "component",
    "homepage": "http://backstrap.github.io/backstrap",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/backstrap/backstrap/issues",
        "source": "https://github.com/backstrap/backstrap/tree/master"
    },
    "authors": [
        {
            "name": "Kevin Perry",
            "email": "perry@princeton.edu"
        }
    ],
    "require": {
        "components/backbone" : "~1.0",
        "components/bootstrap": "~3.1",
        "moment/moment": "dev-develop"
    },
    "require-dev": {
        "components/bootstrap-default": "dev-master"
    },
    "suggest": {
        "components/bootstrap-default": "dev-master"
    },
    "extra": {
        "component": {
            "scripts": [
                "js/requirejs_setup.js",
                "js/backstrap.js",
                "js/input_event.js",
                "js/dispatcher.js",
                "js/LocalCache.js",
                "js/Model.js",
                "js/Collection.js",
                "js/View.js",
                "js/components/closeIcon.js",
                "js/components/dropdown.js",
                "js/components/dropdownGroup.js",
                "js/components/grid.js",
                "js/components/menuItem.js",
                "js/components/menuToggle.js",
                "js/components/nav.js",
                "js/components/navbar.js",
                "js/components/navbarForm.js",
                "js/components/navbarGroup.js",
                "js/mixins/HasAlternativeProperty.js",
                "js/mixins/HasError.js",
                "js/mixins/HasFocus.js",
                "js/mixins/HasFormLabel.js",
                "js/mixins/HasGlyph.js",
                "js/mixins/HasModel.js",
                "js/views/CollectionView.js",
                "js/views/List.js",
                "js/views/Button.js",
                "js/views/Calendar.js",
                "js/views/Select.js",
                "js/views/TextField.js",
                "js/views/Badge.js",
                "js/views/BasicNavbar.js",
                "js/views/Checkbox.js",
                "js/views/Context.js",
                "js/views/Container.js",
                "js/views/DatePicker.js",
                "js/views/Dropdown.js",
                "js/views/DropdownGroup.js",
                "js/views/Glyph.js",
                "js/views/Grid.js",
                "js/views/Label.js",
                "js/views/Link.js",
                "js/views/List.js",
                "js/views/Menu.js",
                "js/views/ModelView.js",
                "js/views/NavPills.js",
                "js/views/NavTabs.js",
                "js/views/Panel.js",
                "js/views/ProgressBar.js",
                "js/views/RadioGroup.js",
                "js/views/Span.js",
                "js/views/Table.js",
                "js/views/TextArea.js",
                "js/views/TimePicker.js",
                "js/requirejs_cleanup.js"
            ],
            "styles": [
               "css/backstrap.css"
            ],
            "shim": {
                "deps": [
                     "jquery",
                     "underscore",
                     "bootstrap",
                     "backbone",
                     "moment"
                ]
            }
        }
    },
    "config": {
        "component": {
            "noGlobal": false
        }
    }
}
