{
    "name": "backstrap/backstrap",
    "description": "The Backstrap JavaScript library provides a layer of functionality tying together Bootstrap and Backbone.",
    "keywords": ["backbone", "bootstrap", "browser", "client", "UI"],
    "type": "component",
    "homepage": "http://backstrap.github.io/backstrap",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/backstrap/backstrap/issues",
        "source": "https://github.com/backstrap/backstrap/tree/master"
    },
    "authors": [
        {
            "name": "Kevin Perry",
            "email": "perry@princeton.edu"
        }
    ],
    "require": {
        "components/backbone" : "~1.0",
        "moment/moment": "~2.9.0"
    },
    "require-dev": {
        "components/bootstrap": "~3.1",
        "components/bootstrap-default": "dev-master",
        "backstrap/moment-interval": "dev-master",
        "backstrap/mobiscroll": "dev-master"
    },
    "suggest": {
        "components/bootstrap": "~3.1",
        "components/bootstrap-default": "dev-master",
        "backstrap/moment-interval": "dev-master",
        "backstrap/mobiscroll": "dev-master"
    },
    "extra": {
        "component": {
            "scripts": [
                "backstrap/core.js",
                "backstrap/input_event.js",
                "backstrap/dispatcher.js",
                "backstrap/LocalCache.js",
                "backstrap/Model.js",
                "backstrap/Collection.js",
                "backstrap/View.js",
                "backstrap/components/closeIcon.js",
                "backstrap/components/dropdown.js",
                "backstrap/components/dropdownGroup.js",
                "backstrap/components/grid.js",
                "backstrap/components/menuItem.js",
                "backstrap/components/menuToggle.js",
                "backstrap/components/nav.js",
                "backstrap/components/navbar.js",
                "backstrap/components/navbarForm.js",
                "backstrap/components/navbarGroup.js",
                "backstrap/mixins/HasAlternativeProperty.js",
                "backstrap/mixins/HasError.js",
                "backstrap/mixins/HasFocus.js",
                "backstrap/mixins/HasFormLabel.js",
                "backstrap/mixins/HasGlyph.js",
                "backstrap/mixins/HasModel.js",
                "backstrap/views/CollectionView.js",
                "backstrap/views/List.js",
                "backstrap/views/Button.js",
                "backstrap/views/Calendar.js",
                "backstrap/views/Select.js",
                "backstrap/views/TextField.js",
                "backstrap/views/Badge.js",
                "backstrap/views/BasicNavbar.js",
                "backstrap/views/Checkbox.js",
                "backstrap/views/Context.js",
                "backstrap/views/Container.js",
                "backstrap/views/DatePicker.js",
                "backstrap/views/DateTime.js",
                "backstrap/views/Dropdown.js",
                "backstrap/views/DropdownGroup.js",
                "backstrap/views/DurationField.js",
                "backstrap/views/FileInput.js",
                "backstrap/views/Glyph.js",
                "backstrap/views/Grid.js",
                "backstrap/views/Label.js",
                "backstrap/views/Link.js",
                "backstrap/views/List.js",
                "backstrap/views/Menu.js",
                "backstrap/views/ModelView.js",
                "backstrap/views/NavPills.js",
                "backstrap/views/NavTabs.js",
                "backstrap/views/NumberField.js",
                "backstrap/views/OnOffGlyph.js",
                "backstrap/views/Panel.js",
                "backstrap/views/ProgressBar.js",
                "backstrap/views/RadioGroup.js",
                "backstrap/views/Span.js",
                "backstrap/views/Table.js",
                "backstrap/views/TextArea.js",
                "backstrap/views/TimePicker.js",
                "backstrap/components.js",
                "backstrap/mixins.js",
                "backstrap/views.js",
                "backstrap/backstrap.js"
            ],
            "styles": [
               "css/backstrap.css"
            ],
            "shim": {
                "deps": [
                     "jquery",
                     "underscore",
                     "backbone",
                     "moment",
                     "moment-interval"
                ]
            }
        }
    },
    "config": {
        "component": {
            "noGlobal": true
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/backstrap/mobiscroll"
        },
        {
            "type": "vcs",
            "url": "https://github.com/backstrap/moment-interval"
        }
    ]
}
