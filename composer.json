{
    "name": "backstrap/backstrap",
    "description": "The Backstrap JavaScript library provides a layer of functionality tying together Bootstrap and Backbone.",
    "keywords": ["backbone", "bootstrap", "browser", "client", "UI"],
    "type": "component",
    "homepage": "https://github.com/backstrap/backstrap",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/backstrap/backstrap",
        "source": "https://github.com/backstrap/backstrap"
    },
    "authors": [
        {
            "name": "Kevin Perry",
            "email": "perry@princeton.edu"
        }
    ],
    "require": {
        "components/backbone" : "~1.0",
        "components/bootstrap": "~3.1",
        "components/bootstrap-default": "~3.0",
        "moment/moment": "2.5.1"
    },
    "extra": {
        "component": {
            "scripts": [
                "src/backstrap/_setup.js",
                "src/backstrap.js",
                "src/input_event.js",
                "src/backstrap/CollectionView.js",
                "src/backstrap/HasAlternativeProperty.js",
                "src/backstrap/HasError.js",
                "src/backstrap/HasFocus.js",
                "src/backstrap/HasFormLabel.js",
                "src/backstrap/HasGlyph.js",
                "src/backstrap/HasModel.js",
                "src/backstrap/List.js",
                "src/backstrap/Badge.js",
                "src/backstrap/BasicNavbar.js",
                "src/backstrap/Button.js",
                "src/backstrap/Calendar.js",
                "src/backstrap/Checkbox.js",
                "src/backstrap/Context.js",
                "src/backstrap/DatePicker.js",
                "src/backstrap/Dropdown.js",
                "src/backstrap/Glyph.js",
                "src/backstrap/Label.js",
                "src/backstrap/Link.js",
                "src/backstrap/List.js",
                "src/backstrap/Menu.js",
                "src/backstrap/NavPills.js",
                "src/backstrap/NavTabs.js",
                "src/backstrap/RadioGroup.js",
                "src/backstrap/Table.js",
                "src/backstrap/TextArea.js",
                "src/backstrap/TextField.js",
                "src/backstrap/TimePicker.js",
                "src/backstrap/_cleanup.js"
            ],
            "styles": [
               "css/backstrap.css"
            ],
            "shim": {
                "deps": [
                     "jquery",
                     "underscore",
                     "bootstrap",
                     "backbone",
                     "moment"
                ]
            }
        }
    },
    "config": {
        "component": {
            "noGlobal": false
        }
    }
}
